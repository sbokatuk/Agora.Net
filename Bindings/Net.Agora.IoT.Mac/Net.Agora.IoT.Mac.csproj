<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Version>1.0.0.8</Version>
    <authors>s.bokatuk</authors>
    <owners>s.bokatuk</owners>
    <title>Xamarin bindings to the native Agora IoT Mac SDK.</title>
    <projectUrl>https://github.com/sbokatuk/Net.Agora</projectUrl>
    <description>MAUI .Net Mac Xamarin bindings for Agora IoT SDK. Alows to use The Agora IoT SDK in your app.</description>
    <summary>.Net Xamarin Agora IoT Mac SDK, see https://www.agora.io/en/</summary>
    <tags>Xamarin Agora .Net IoT Mac SDK Xamarin Xamarin.Mac MAUI MAUI.Mac .Net Binding</tags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageId>Net.Agora.IoT.Mac</PackageId>
    <AssemblyName>Net.Agora.IoT.Mac</AssemblyName>
    <SynchReleaseVersion>false</SynchReleaseVersion>
    <ReleaseVersion>4.2.6.1</ReleaseVersion>
    <TargetFramework>net8.0-maccatalyst</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>true</ImplicitUsings>
    <IsBindingProject>true</IsBindingProject>
  </PropertyGroup>
  <ItemGroup>
    <None Include="README.md" Pack="true" PackagePath="\" />
    <ObjcBindingApiDefinition Include="ApiDefinition.cs" />
    <ObjcBindingCoreSource Include="StructsAndEnums.cs" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="Net.Agora.IoT.Mac.targets">
      <PackagePath>build/Net.Agora.IoT.Mac.targets</PackagePath>
    </Content>
  </ItemGroup>
</Project>
